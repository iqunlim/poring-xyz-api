{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "poring-xyz-api",
  "compatibility_date": "2025-01-29",
  "compatibility_flags": ["nodejs_compat"],
  "pages_build_output_dir": ".vercel/output/static",
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "binding": "IMAGES",
      "bucket_name": "poring-xyz"
    }
  ],
  "vars": {
    "MAX_FILE_SIZE": 100
  }
}
